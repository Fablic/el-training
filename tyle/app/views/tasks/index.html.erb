<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>
<table>
  <tr>
    <th>name</th>
    <th>description</th>
    <th>priority</th>
    <th>status</th>
    <th>due</th>
  </tr>
  <% @tasks.each do |task| %>
  <tr>
    <td><%= task.name %></td>
    <td><%= task.description %></td>
    <td><%= task.priority %></td>
    <td><%= task.status %></td>
    <td><%= task.due %></td>
  </tr>
  <% end %>
</table>
