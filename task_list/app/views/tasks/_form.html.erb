<%= form_with(model: @task, local: true) do |form| %>
	<%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :content %>
  <%= form.text_field :content %>
	<%= form.label :priority %>
  <%= form.text_field :priority %>
  <%= form.label :status %>
  <%= form.text_field :status %>
  <%= form.label :endtime %>
  <%= form.text_field :endtime %>
  <%= form.submit %>
<% end %>